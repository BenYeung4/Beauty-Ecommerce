{{!Cart File}}


<div>Checkout</div>
<div></div>
<div class="m-3">
  {{#each cart as |item|}}
  <a href="#">
  <div class="w-96 h-48 object-contain m-3 p-1 rounded-md bg-slate-100 hover:bg-slate-200">
    <div class="float-right w-48"><img src="{{item.product.url}}" class="object-contain h-12 w-24" /></div>
    <div class="w-full mx-auto">{{item.product.name}}</div>
  </div>
  </a>
  {{/each}}
</div>

<script src="/javascript/cart.js"></script>